{
    "manifest_version": 3,
    "name": "Dark mode",
    "description": "Set website to dark mode, for personal use",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_icon": {
            "16": "images/moon-16.png",
            "32": "images/moon-32.png",
            "48": "images/moon-48.png",
            "128": "images/moon-128.png"
        },
        "default_title": "Set dark mode"
    },
    "background": {
        "service_worker": "background.js"
    }
}